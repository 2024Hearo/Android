<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <inset android:insetTop="-2dp" android:insetRight="-2dp" android:insetLeft="-2dp">
            <shape android:shape="rectangle">
                <solid android:color="@android:color/transparent" /> <!-- 배경색: 투명 -->
                <stroke android:width="1.5dp" android:color="@color/white" /> <!-- 선 색상: 흰색 -->
            </shape>
        </inset>
    </item>
</selector>
